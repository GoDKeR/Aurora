[Properties.Blend]

	ColorSrcFactor  = "One"
	AlphaSrcFactor  = "One"

	ColorDstFactor  = "OneMinusSrcAlpha"
	AlphaDstFactor  = "OneMinusSrcAlpha"

[Properties.Depth]

	Mask            = 0
	Condition       = "LessEqual"

[Properties.Layout]

	Attributes      = [
		[0, 0, 0,  "Float32x3"  ],
		[1, 0, 12, "UIntNorm8x4"],
		[2, 0, 16, "Float32x2"  ]
	]

[Program.Vertex]

	Entry    = "vertex"
	Filename = "Engine://Pipeline/UI.shader"

[Program.Fragment]

	Entry    = "fragment"
	Filename = "Engine://Pipeline/UI.shader"